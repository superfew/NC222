from logic import *
import termcolor
onKitchenTable = Symbol("เงินตั้งอยู่บนโต๊ะในห้องทำงาน")
sawAtBreakfast = Symbol("เห็นโทรศัพท์ในตอนเช้า")
readNewsInLivingRoom = Symbol("กำลังนั่งเล่นเกมในห้องนอน")
readNewsInKitchen = Symbol("กำลังเล่นเกมในห้องน้ำ")
onCoffeeTable = Symbol("เงินตั้งอยู่บนโต๊ะกินข้าว")
readBookInBed = Symbol("กำลังเล่นเกมอยู่ที่โต๊ะกินข้าว")
onBedTable = Symbol("เงินอยู่ในห้องนอน")
knowledge = And(
    Implication(อยู่บนโต๊ะทำงาน, ในตอนเช้า),
    Or(เล่นเกมในห้องนอน, เล่นเกมในห้องน้ำ),
    Implication(เล่นเกมในห้องนอน, บนโต๊ะกินข้าว),
    Not(เห็นในตอนเช้า),
    Implication(เล่นเกมที่โต๊ะกินข้าว, ในห้องนอน),
    Implication(เล่นเกมในห้องน้ำ, ในอ่างอาบน้ำ)
)
symbols = [บนโต๊ะกินข้าว, ในห้องทำงาน, ในห้องนอน]
def check_knowledge(knowledge):
    for s in symbols:
        if model_check(knowledge, s):
            termcolor.cprint(f"{s}", "green")

check_knowledge(knowledge)
